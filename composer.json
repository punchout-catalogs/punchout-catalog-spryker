{
  "name": "punchout-catalogs/punchout-catalog-spryker",
  "description": "Punchout Catalog Module for Spryker eCommerce Platform",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "spryker/kernel": "^3.0.0",
    "spryker-shop/customer-page": "*",
    "spryker/cart": "^4.3.0 || ^5.0.0",
    "spryker/glossary-storage": "^1.0.0",
    "spryker/quote": "^1.1.0 || ^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "PunchoutCatalog\\": "src/PunchoutCatalog/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
