{
  "customer": {
    "fields": {
      "first_name": {
        "path": "/cXML/Request[1]/PunchOutSetupRequest[1]/Contact[1]/Name[1]",
        "transform": [{
          "split": {
            "sep": "\\s",
            "index": "1"
          }
        }]
      },
      "last_name": {
        "path": "/cXML/Request[1]/PunchOutSetupRequest[1]/Contact[1]/Name[1]",
        "transform": [{
          "split": {
            "sep": "\\s",
            "index": "2"
          }
        }]
      },
      "email": {
        "path": "/cXML/Request[1]/PunchOutSetupRequest[1]/Contact[1]/Email[1]"
      },
      "salutation": {
        "path": "/cXML/Request[1]/PunchOutSetupRequest[1]/Contact[1]/Salutation_fake_node[1]",
        "transform": [{
          "default": {
            "value": "Mr"
          }
        }]
      }
    }
  },

  "cart_item": {
    "multi_lines": {
      "path": "/cXML/Request[1]/PunchOutSetupRequest[1]/ItemOut"
    },
    "fields": {
      "internal_id":{
        "path": "ItemID[1]/SupplierPartAuxiliaryID[1]"
      }
    }
  }
}